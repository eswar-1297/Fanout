{
  "name": "fanout-query-generator",
  "version": "1.0.0",
  "description": "AI-powered fanout query generation tool",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/index.ts",
    "dev:client": "cd client && npm run dev",
    "build": "tsc && cd client && npm run build",
    "start": "node dist/index.js",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": ["fanout", "queries", "ai", "search", "llm"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}

